  
  <div id="exTab1" class="container">	
    
    <div id="myBtnContainer">
      {{#each listTypes}}
      <button class="btn{{#if this.ACTIVE_YN}} active{{/if}}" onclick="document.location.href='/Applist?listType={{this.listId}}'">{{this.listName}}</button>
      {{/each}}
    </div><br/><br/>

  <div id="test-list" >
    <ul class="product-list-basic list" id="list">
      {{#each res}}
      <li >
        <a href="#" class="product-photo">
            <img src="{{{this.icon}}}" height="130" alt="BlueStack" />
        </a>
        <h2><a href="#">{{{this.title}}}</a></h2>
        
        <div class="product-rating">
          <div>
            <span class="product-stars" style="width: 60px" >
              <a href="#">{{{this.category}}}</a>
            </span>
          </div>
          <span class="product-price" style="font-size: 15px;"><a href="#">{{{this.version}}}</a></span>
        </div>
        <p class="product-description">{{{this.package_name}}}</p>
        <button onclick="document.location.href='/appdetails?pkg={{this.package_name}}'">Details</button>
        <p class="product-price" >{{{this.rating}}}</p>

      </li>
      {{/each}}
    </ul>
    <ul class="pagination"></ul>
  </div>
</div>
</body>

